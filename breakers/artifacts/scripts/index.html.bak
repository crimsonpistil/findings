    jQuery('.color-scheme-list > div > h2').click(function(){
        var hzCL = jQuery(this).parent().hasClass('active');
        if(hzCL == false){
            jQuery('.color-scheme-list div').removeClass('active').find('div').slideUp(200);
            jQuery(this).parent().find('div:first').slideDown(400);
            jQuery(this).parent().addClass('active');
        }else{
            jQuery(this).parent().find('div:first').slideUp(400);
            jQuery(this).parent().removeClass('active');
        }
    });
    
