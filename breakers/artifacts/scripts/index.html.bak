function displayPopup() {
  if (localStorage.getItem("isUserSubscribed")) {
	  document.getElementById('card-newsletter').style.display = 'none';
	} else {
		document.getElementById('card-newsletter').style.display = 'inline-block';
		localStorage.setItem("isUserSubscribed", false);
	}
}

function closePopup() {
  document.getElementById('card-newsletter').style.display = 'none';
  localStorage.setItem("isUserSubscribed", true);
}

function myFunction() {
	closePopup();
}

